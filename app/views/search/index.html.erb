<h3><%= @storesearch.store_count %> Total Stores</h3>

<% @storesearch.stores.each do |store| %>
  <ul class='store'>
    <li class='name'>Name: <%= store.name %></li>
    <li class='city'>City: <%= store.city %></li>
    <li class='distance'>Distance: <%= store.distance %> miles</li>
    <li class='phone'>Phone Number: <%= store.phone %></li>
    <li class='store-type'>Store Type: <%= store.store_type %></li>
  </ul>
<% end %>
